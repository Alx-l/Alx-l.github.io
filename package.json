{
  "name": "bubbly-boilerplate",
  "version": "1.1.0",
  "description": "A bubbly boilerplate",
  "main": "index.js",
  "scripts": {
    "init-app": "yarn && mkdir public && yarn move-files && webpack --config build/dev/webpack.config.dev.js",
    "start": "yarn watch",
    "watch": "npm-run-all html-build move-files -p webpack-dev",
    "build": "yarn webpack-prod",
    "build-ftp": "npm-run-all html-build webpack-prod",
    "move-files": "cp -r ./fonts ./images ./public",
    "html-build": "jade jade/views/*.jade --out public",
    "webpack-dev": "webpack-dev-server -w --progress --colors --config build/dev/webpack.config.dev.js",
    "webpack-prod": "webpack --config build/prod/webpack.config.prod.js"
  },
  "author": "Alex L",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.11.0",
    "jade": "^1.11.0"
  },
  "devDependencies": {
    "1-liners": "^0.4.0",
    "abyssa": "^8.0.3",
    "animejs": "^2.0.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.1",
    "css-mqpacker": "^4.0.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fluxx": "^0.14.14",
    "immupdate": "^1.0.0",
    "npm-run-all": "^3.1.1",
    "postcss": "^5.2.5",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-flexbox": "^1.0.2",
    "postcss-import": "^9.1.0",
    "postcss-inline-svg": "^2.3.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^1.3.3",
    "postcss-mixins": "^5.4.1",
    "postcss-pxtorem": "^3.3.1",
    "postcss-reporter": "^3.0.0",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "rucksack-css": "^0.9.1",
    "style-loader": "0.13.2",
    "stylelint": "^7.9.0",
    "stylelint-order": "^0.4.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
