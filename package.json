{
  "name": "bubbly-boilerplate",
  "version": "1.1.0",
  "description": "A bubbly boilerplate",
  "main": "index.js",
  "scripts": {
    "browsersync": "browser-sync start --config ./conf/bs-config.js",
    "build": "npm-run-all styles-prod webpack-prod",
    "build-ftp": "npm-run-all html-build styles-prod webpack-prod",
    "create-app": "mkdir public && mkdir public/css && touch public/css/pre-bundle.css && mkdir public/js && npm run move-files && webpack --config conf/webpack.config.dev.js",
    "html-build": "jade jade/views/*.jade --out public",
    "move-files": "cp -r ./fonts ./images ./public",
    "styles-dev": "postcss --config ./conf/postcss.dev.js",
    "styles-prod": "postcss --config ./conf/postcss.prod.js",
    "start": "npm run watch",
    "watch": "npm-run-all html-build move-files -p styles-dev webpack-dev browsersync",
    "webpack-dev": "webpack -w --inline --config conf/webpack.config.dev.js",
    "webpack-prod": "NODE_ENV=production webpack --config conf/webpack.config.prod.js"
  },
  "author": "Alex L",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.11.0",
    "express": "^4.13.3",
    "jade": "^1.11.0"
  },
  "devDependencies": {
    "abyssa": "^8.0.3",
    "animejs": "^2.0.0",
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.6.0",
    "classnames": "^2.2.5",
    "css-mqpacker": "^4.0.0",
    "cssnano": "^3.3.1",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^5.0.1",
    "fluxx": "^0.14.14",
    "immupdate": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.5",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-flexbox": "^1.0.2",
    "postcss-import": "^9.1.0",
    "postcss-inline-svg": "^2.3.0",
    "postcss-mixins": "^5.4.1",
    "postcss-pxtorem": "^3.3.1",
    "postcss-reporter": "^3.0.0",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "rucksack-css": "^0.9.1",
    "webpack": "^1.12.14"
  }
}
